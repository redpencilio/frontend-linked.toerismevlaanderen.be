{{!-- Page to list contact-points --}}
<div class="content">
  <h2 class="h2">{{show-models-link}} &lt; {{t (humanize 'contact-points')}}</h2>
  {{#data-table
     content=model
     isLoading=isLoadingModel
     filter=filter
     sort=sort
     page=page
     size=size
     as |table|}}
    {{#table.content as |c|}}
      {{#c.header}}
        {{th-sortable field='honorificPrefix' currentSorting=sort label=(t 'honorificPrefix')}}
{{th-sortable field='email' currentSorting=sort label=(t 'email')}}
{{th-sortable field='fax' currentSorting=sort label=(t 'fax')}}
{{th-sortable field='name' currentSorting=sort label=(t 'name')}}
{{th-sortable field='page' currentSorting=sort label=(t 'page')}}
{{th-sortable field='telephone' currentSorting=sort label=(t 'telephone')}}
        
        <th>{{!-- contains detail view action --}}</th>
        <th>{{!-- contains detail edit action --}}</th>
      {{/c.header}}
      {{#c.body as |row|}}
        {{#with-rdfa-context tagName="td" model=row as |ctx|}}
                {{#each row.honorificPrefix as |langString|}}
                  <span property={{row.rdfaBindings.honorificPrefix}} lang="{{langString.language}}" content="{{langString.content}}">
                    <span class="language-string-set__content">"{{langString.content}}"</span><span class="language-string-set__language">@{{langString.language}}</span>
                  </span>
                {{/each}} {{/with-rdfa-context}}
{{#with-rdfa-context tagName="td" model=row as |ctx|}}{{ctx.get prop=email}}{{/with-rdfa-context}}
{{#with-rdfa-context tagName="td" model=row as |ctx|}}{{ctx.get prop=fax}}{{/with-rdfa-context}}
{{#with-rdfa-context tagName="td" model=row as |ctx|}}{{ctx.get prop=name}}{{/with-rdfa-context}}
{{#with-rdfa-context tagName="td" model=row as |ctx|}}{{ctx.get prop=page}}{{/with-rdfa-context}}
{{#with-rdfa-context tagName="td" model=row as |ctx|}}{{ctx.get prop=telephone}}{{/with-rdfa-context}}
                
                {{#with-rdfa-context tagName="td" model=row as |ctx|}}{{#rdfa/link-to link-to='contact-points.show' value=row property="foaf:page"}}{{t "Open"}}{{/rdfa/link-to}}{{/with-rdfa-context}}
        
      {{/c.body}}
    {{/table.content}}
  {{/data-table}}

  
</div>
