{{!-- Page to show details of registration-lodging-types --}}
{{#with-rdfa-context class="container" model=model as |ctx|}}
  <div class="page">
    <h2 class="h2">{{link-to (t "registration-lodging-type overview") "registration-lodging-types.index"}} &lt; {{t (humanize 'registration-lodging-type details')}}</h2>
    <div class="properties">
      <div class="properties__title">
        {{t (humanize 'registration-lodging-type data')}}
        <dl>
          <dt class="properties__label">URI</dt>
          <dd class="properties__data">{{#link-to "registration-lodging-types.show" model href=model.uri}}{{model.uri}}{{/link-to}}</dd>
          <dt class="properties__label">{{t "label"}}</dt> <dd class="properties__data">
                {{#each model.label as |langString|}}
                  <span property={{model.rdfaBindings.label}} lang="{{langString.language}}" content="{{langString.content}}">
                    <span class="language-string-set__content">"{{langString.content}}"</span><span class="language-string-set__language">@{{langString.language}}</span>
                  </span>
                {{/each}}
              </dd>
          <dt class="properties__label">{{t "registration"}}</dt>
          <dd class="properties__data">{{#ctx.get prop="registration" useUri=true link-to="registrations.show" as |ctx val|}}{{val.stringRep}}{{/ctx.get}}</dd>
<dt class="properties__label">{{t "registrationPublicationType"}}</dt>
          <dd class="properties__data">{{#ctx.get prop="registrationPublicationType" useUri=true link-to="registration-publication-lodging-types.show" as |ctx val|}}{{val.stringRep}}{{/ctx.get}}</dd>
          
        </dl>
      </div>
    </div>
  </div>

  {{!-- Actions to go back to the list-view or edit registration-lodging-type if it's an editable application. --}}
  <div class="actions">
    <div class="button-group">
      {{#link-to "registration-lodging-types" class="button" title="Return to list" tagName='button'}}{{t "< Return to the list"}}{{/link-to}}
      
    </div>
  </div>
{{/with-rdfa-context}}
