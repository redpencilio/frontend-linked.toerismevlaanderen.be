{{!-- Page to show details of generations --}}
{{#with-rdfa-context class="container" model=model as |ctx|}}
  <div class="page">
    <h2 class="h2">{{link-to (t "generation overview") "generations.index"}} &lt; {{t (humanize 'generation details')}}</h2>
    <div class="properties">
      <div class="properties__title">
        {{t (humanize 'generation data')}}
        <dl>
          <dt class="properties__label">{{t "timestamp"}}</dt>
          <dd class="properties__data">{{ctx.get prop="timestamp"}}</dd>
<dt class="properties__label">{{t "activity"}}</dt>
          <dd class="properties__data">{{ctx.get prop="activity"}}</dd>
          
          
        </dl>
      </div>
    </div>
  </div>

  {{!-- Actions to go back to the list-view or edit generation if it's an editable application. --}}
  <div class="actions">
    {{#link-to "generations" class="button" title="Return to list" tagName='button'}}{{t "< Return to the list"}}{{/link-to}}
    
  </div>
{{/with-rdfa-context}}
