{{!-- Page to list rental-units --}}
<div class="content">
  <h2 class="h2">{{show-models-link}} &lt; {{t (humanize 'rental-units')}}</h2>
  {{#data-table
     content=model
     isLoading=isLoadingModel
     filter=filter
     sort=sort
     page=page
     size=size
     as |table|}}
    {{#table.content as |c|}}
      {{#c.header}}
        
        
        <th>{{!-- contains detail view action --}}</th>
        
      {{/c.header}}
      {{#c.body as |row|}}
        
                
                {{#with-rdfa-context tagName="td" model=row as |ctx|}}{{#rdfa/link-to link-to='rental-units.show' value=row property="foaf:page"}}{{t "Open"}}{{/rdfa/link-to}}{{/with-rdfa-context}}
        
      {{/c.body}}
    {{/table.content}}
  {{/data-table}}

  
</div>
